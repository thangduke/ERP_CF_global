<?xml version="1.0" encoding="utf-8"?> 
<templates> 
    <t t-name="employee_button.ListView.Buttons" t-inherit="web.ListView.Buttons">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="after">
            <button type="button" class="btn btn-primary" style="margin-left: 10px;" t-on-click="OnImportClick">
                <i class="fa fa-upload me-1"/>
                Import
            </button>
            <button type="button" class="btn btn-secondary" style="margin-left: 5px;" t-on-click="OnExportClick">
                <i class="fa fa-download me-1"/>
                Export
            </button>
        </xpath>
    </t>
</templates>